          <gmd:title>
            <gco:CharacterString>ERA5-Land, Total Precipitation</gco:CharacterString>
          </gmd:title>
          <gmd:identifier>
            <gmd:MD_Identifier/>
          </gmd:identifier>
        </gmd:CI_Citation>
